#include "main.h"
#include <stdio.h>
/**
 * main - entry point
 * Description: create a file with a crackme2 password
 * Return: 1 for error, 0 for password
*/
int main(void)
{
	FILE *file

	file = fopen("password.txt", "W");
	if (file == NULL)
	{
		printf("Error opening file!");
			return (1);
	}

	fprintf(file, "YourPassword");

	fclose(file);

	return (0);
}
